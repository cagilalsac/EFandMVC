@model User
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    ViewData["Title"] = "User Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    @* 
        User tipindeki modeli _Details partial view'ına göderiyoruz
        ve kullanıcı detay verilerinin partial view'da yazdırılmasını sağlıyoruz
    *@
    <partial name="_Details" model="Model" />
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>&nbsp;|&nbsp;
    <a asp-action="Index">Back to List</a>
</div>



